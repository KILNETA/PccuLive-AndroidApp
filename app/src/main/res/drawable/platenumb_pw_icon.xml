<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="3dp"
        android:left="10dp"
        android:height="8dp"
        android:width="8dp">
        <rotate
            android:fromDegrees="45">
            <shape>
                <solid android:color="#a3cc49" />
            </shape>
        </rotate>
    </item>

    <item
        android:bottom="10dp"
        android:top="5dp">
        <shape>
            <solid android:color="#a3cc49" />
            <corners
                android:topLeftRadius="4dp"
                android:topRightRadius="4dp" />
        </shape>
    </item>


    <item
        android:top="10dp">
        <shape>
            <solid android:color="#404040" />
            <corners
                android:bottomLeftRadius="4dp"
                android:bottomRightRadius="4dp" />
        </shape>
    </item>

    <item
        android:top="10dp"
        android:right="1dp"
        android:left="1dp"
        android:bottom="1dp">
        <shape>
            <solid android:color="#303030" />
            <corners
                android:bottomLeftRadius="4dp"
                android:bottomRightRadius="4dp" />
        </shape>
    </item>

</layer-list>