<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- rectangle表示为矩形 -->

    <!-- 边框的颜色和粗细 -->
    <stroke
        android:width="2px"
        android:color="#F3F3F3"
        />

    <solid android:color="#FFFFFF"/>   <!-- 设置背景颜色 -->

    <!-- android:radius 圆角的半径 -->
    <corners
        android:radius="75dp"
        />
</shape>